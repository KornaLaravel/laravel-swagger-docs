{
  "name": "ronasit/laravel-swagger",
  "description": "Provided middleware for generating of swagger-documentation file by run testing of restful API.",
  "keywords": [
    "laravel",
    "swagger",
    "testing",
    "auto-documentation",
    "rest-api"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Ronas IT",
      "homepage": "https://github.com/RonasIT/laravel-swagger/graphs/contributors"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "laravel/framework": ">=5.3.0",
    "phpunit/phpunit": ">=7.0|<=10.0",
    "ext-json": "*",
    "ext-curl": "*"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "1.4.*",
    "orchestra/testbench": "^6.24",
    "guzzlehttp/guzzle": "^7.4"
  },
  "autoload": {
    "psr-4": {
      "RonasIT\\Support\\AutoDoc\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ],
    "exclude-from-classmap": [
      "src/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "RonasIT\\Tests\\": "tests/"
    },
    "files": [
      "tests/TestCase.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "RonasIT\\Support\\AutoDoc\\AutoDocServiceProvider"
      ]
    }
  }
}
